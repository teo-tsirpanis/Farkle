    <Compile Include="Grammar/GrammarReader.fs" />
    <Compile Include="Grammar/EGT.fs" />
    <Compile Include="Builder/SetUtils.fs" />
    <Compile Include="Parser/AST.fs" />
    <Compile Include="PostProcessor/Transformer.fs" />
    <Compile Include="PostProcessor/Fuser.fs" />
    <Compile Include="PostProcessor/PostProcessor.fs" />
    <Compile Include="Parser/ParserTypes.fs" />
    <Compile Include="Parser/Tokenizer.fs" />
