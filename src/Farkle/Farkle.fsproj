<?xml version="1.0" encoding="utf-8"?>
<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFrameworks>net45;netstandard2.0</TargetFrameworks>
    <Title>The Farkle Parser</Title>
    <PackageId>Farkle</PackageId>
    <Product>Farkle</Product>
